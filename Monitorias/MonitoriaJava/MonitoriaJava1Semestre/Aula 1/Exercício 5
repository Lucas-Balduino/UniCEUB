import java.util.Scanner;

public class MyClass {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int a, b, c, d, diferencaProduto;
        System.out.println("Digite os valores de a, b, c e d: ");
        
        a = sc.nextInt();
        b = sc.nextInt();
        c = sc.nextInt();
        d = sc.nextInt();
        
        diferencaProduto = ((a * b) - (c * d));
        
        System.out.println("A diferenca dos produtos de a, b, c e d e "+ diferencaProduto);
    }
}
