package main;
import java.util.Scanner;


/*Criem um programa que recebe o número X e se o número não for  10 ele vai transformá lo em 10  */


public class Exercicio_2_Aula_2 {
   public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
       int x;


       System.out.println("Digite um numero: ");
       x = sc.nextInt();
      
       System.out.println("Seu numero é "+ x);
      
       if (x != 10){
           x = 10;


           System.out.println("Agora seu numero é "+ x);
       }
      
       else {
           System.out.println("Seu numero é 10");
       }
   }
}
